	"husky": {
		"hooks": {
			"pre-commit": "lint-staged && npm run tsc"
		}
	},
	"lint-staged": {
		"**/translations_data.json": [
			"node ./scripts/import_translations.js --format",
			"git add"
		],
		"*.js|*.scss|*.jsx|*.json": [
			"npm run format",
			"git add"
		],
		"*.ts|*.tsx": [
			"npm run format",
			"npm run lint:fix",
			"git add"
		]
	},
	"jest": "<rootDir>jest.config.js",
	"detox": {
		"configurations": {
			"ios": {
				"binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/Omantel.app",
				"build": "react-native start --port 8081 & xcodebuild -workspace ios/Omantel.xcworkspace -scheme Omantel -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
				"type": "ios.simulator",
				"device": {
					"type": "iPhone 11"
				}
			},
			"android": {
				"binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
				"build": "cd android && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug && cd ..",
				"type": "android.emulator",
				"device": {
					"avdName": "Pixel_3_XL_API_29"
				}
			}
		},
		"test-runner": "jest"
	}
