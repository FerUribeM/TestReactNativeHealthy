{
	"name": "omantel",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"start": "react-native start",
		"android": "react-native run-android --appIdSuffix dev",
		"ios": "react-native run-ios",
		"test": "jest --no-cache --verbose --detectOpenHandles --forceExit",
		"test:ci": "jest --ci --reporters=default --reporters=jest-junit",
		"test:watch": "jest --watch",
		"test:watchAll": "jest --watchAll",
		"lint": "eslint ./src --ext .ts,.tsx",
		"lint:fix": "eslint ./src --ext .ts,.tsx --fix",
		"format": "prettier --config .prettierrc \"**/*.{json,js,ts,jsx,tsx}\" --write",
		"format:check": "prettier --config .prettierrc \"**/*.{json,js,ts,jsx,tsx}\" --check",
		"reverseTCP": "adb reverse tcp:8081 tcp:8081",
		"clean:android": "cd android && gradlew.bat clean",
		"build:ios": "xcodebuild -workspace ios/Omantel.xcworkspace -scheme Omantel -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
		"build:android": "cd android && ./gradlew assembleDebug",
		"e2e:ios": "cat .env.test > .env && detox build -c ios && detox test --take-screenshots=all --record-videos=all -a artifacts -c ios --detectOpenHandles --forceExit",
		"e2e:android": "cat .env.test > .env && detox build -c android && detox test --take-screenshots=all --record-videos=all -a artifacts -c android",
		"e2e:ios:stg": "cat .env.test.stg > .env && detox build -c ios && detox test --take-screenshots=all --record-videos=all -a artifacts -c ios",
		"e2e:android:stg": "cat .env.test.stg > .env && detox build -c android && detox test --take-screenshots=all --record-videos=all -a artifacts -c android",
		"tsc": "tsc --project ."
	},
